type Channels @pgTable(name: "channels") {
  id: ID! @unique
  channelname: String!
  guild: Guilds @pgRelation(column: "guild_id")
  type: Int!
}

type Guilds @pgTable(name: "guilds") {
  id: ID! @unique
  channels: [Channels!]!
  guildname: String!
  members: [Members!]!
  owner: Users @pgRelation(column: "owner")
}

type Members @pgTable(name: "members") {
  id: ID! @unique
  guild: Guilds @pgRelation(column: "guild_id")
  user: Users @pgRelation(column: "user_id")
}

type Users @pgTable(name: "users") {
  id: ID! @unique
  email: String! @unique
  guilds: [Guilds!]!
  members: [Members!]!
  password: String!
  username: String! @unique
}